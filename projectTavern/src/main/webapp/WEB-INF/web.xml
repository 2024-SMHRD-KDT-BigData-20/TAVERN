<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet-mapping>
		<servlet-name>JoinCon</servlet-name>
		<url-pattern>/JoinCon</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateCon</servlet-name>
		<url-pattern>/UpdateCon</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Find_pwCon</servlet-name>
		<url-pattern>/Find_pwCon</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>5</session-timeout> <!-- 세션 유효 시간을 5분으로 설정 -->
	</session-config>
</web-app>